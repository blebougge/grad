<PROG>      := <COM> prog ID ; <CONST> <BLOCK> | <COM> prog ID ; <BLOCK>
<COM>       := # ID 
<BLOCK>     := <BEGIN> <BODY> <END>
<BEGIN>     := begin ;
<END>       := end ;
<BODY>      := <EXPR> | <IF> | <WHILE> | &
<EXPR>      := <DCLR_VAR> | <FLN>
<FLN>       := <FUNC> ; | <LOGIC> ; | <NUMERIC> ;
<LOGIC>     := ID <LOGIC'>
<LOGIC'>    := LOG ID <LOGIC'> | &
<NUMERIC>   := ID = ID <NUMERIC'> 
<NUMERIC'>  := OP ID <NUMERIC'> | &
<FUNC>      := <FUNC_NAME>(<PARAM>) | <FUNC_NAME>()
<FUNC_NAME> := write | read
<PARAM>     := ID <PARAM'>
<PARAM'>    := , ID <PARAM'> | &
<IF>        := if(<LOGIC>) ; <BLOCK>
<WHILE>     := while(<LOGIC>) ; <BLOCK>
<CONST>     := fix <DCLR_VAR>
<DCLR_VAR>  := TYPE:ID = ID ;
